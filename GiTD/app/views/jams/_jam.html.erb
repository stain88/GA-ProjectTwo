<div class="col-md-4 text-center">
  <%= link_to jam_path(jam.id) do %>
    <div class="thumbnail tn-custom tn-jam">
      <h1>GiTD #<%= jam.title %></h1>
      <h3>Theme: <%= jam.theme %></h3>
      <h3>Mechanic: <%= jam.mechanic %></h3>
      <h5>Status: <%= jam.status %></h5>
    </div>
  <% end %>
</div>